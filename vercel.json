{
  "builds": [
    {
      "src": "back-end/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/dados",
      "destination": "/back-end/index.js"
    },
    {
      "source": "/historico",
      "destination": "/back-end/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/front-end/$1"
    }
  ]
}